cmake_minimum_required(VERSION 2.8)
project(Screen_decoration)
find_package(OpenCV REQUIRED)
include_directories(${OpenCV_INCLUDE_DIRS})
include_directories("/home/amit/RPLIDAR/rplidar_sdk_v1.5.4/sdk/sdk/include")
add_executable(Screen_decoration screen_deco.cpp)
find_library(RPLIDAR_LIBRARY rplidar_sdk HINTS /home/amit/RPLIDAR/rplidar_sdk_v1.5.4/sdk/output/Linux/Release)
target_link_libraries(Screen_decoration ${OpenCV_LIBS})
target_link_libraries(Screen_decoration m)
target_link_libraries(Screen_decoration PUBLIC ${RPLIDAR_LIBRARY})

